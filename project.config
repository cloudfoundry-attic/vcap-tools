[receive]
	requireChangeId = true
[access "refs/*"]
	owner = group Administrators
	forgeAuthor = group Administrators
	forgeCommitter = group Administrators
	forgeServerAsCommitter = group Administrators
[access "refs/heads/*"]
	create = group Administrators
	push = +force group Administrators
	label-Code-Review = -2..+2 group Platforms Team
	label-Code-Review = -2..+2 group Services Committers
	submit = group vcap-services_developers
	label-Verified = -1..+1 group vcap-services_developers
